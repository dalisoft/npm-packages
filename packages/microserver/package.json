{
  "name": "microhelpers",
  "version": "0.0.1",
  "description": "Pack of micro-helpers for uWebSockets.js",
  "main": "./cjs/index.cjs",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs",
      "default": "./esm/index.js"
    }
  },
  "module": "./esm/index.js",
  "type": "module",
  "typings": "microhelpers.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint ./esm",
    "prepublishOnly": "npm run lint"
  },
  "engines": {
    "node": ">=12.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/microhelpers"
  },
  "keywords": [
    "ast-parser",
    "nanoexpress"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/microhelpers/issues"
  },
  "homepage": "https://github.com/dalisoft/microhelpers#readme",
  "dependencies": {
    "cookie": "^1.0.2"
  }
}
